{
  "targets": ["cursor"],
  "rulesDir": "./rules",
  "mcpServers": {
    "shared-server": {
      "command": "./scripts/aicm-updated-server.sh",
      "args": ["--aicm-updated"],
      "env": { "AICM_TOKEN": "aicm-new-token" }
    },
    "aicm-only-server": {
      "command": "./scripts/aicm-only.sh",
      "env": { "AICM_ONLY": "true" }
    }
  }
}
